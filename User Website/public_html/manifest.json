{
  "name": "Punch Clock",
  "version": "1.3",
  "manifest_version": 3,
  "action": {
    "default_popup": "pc.html",
    "default_title": "Punch Clock"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "libs/jquery/jquery-3.3.1.js",
        "punchclock.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js",
    "type":"module"
  },
  "icons": {
    "16": "/icons/16x16.png",
    "48": "/icons/48x48.png",
    "128": "/icons/128x128.png"
  },
  "description": "Punch Clock",
  "permissions": [
    "storage"
  ],
  "host_permissions":[
  ],
  "web_accessible_resources": [{
    "resources":[],
    "matches":["<all_urls>"]
  }]
}